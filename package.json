{
    "name": "my-project",
    "version": "1.0.0",
    "scripts": {
      "start": "concurrently \"npm run dev:frontend\" \"npm run start:backend\"",
      "dev:frontend": "cd frontend && npm run dev",
      "start:backend": "cd backend && npm start",
      "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
    },
    "dependencies": {
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "devDependencies": {
      "concurrently": "^8.2.2"
    }
  }